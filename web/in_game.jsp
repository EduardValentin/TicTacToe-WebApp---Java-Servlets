<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet">
        <title>Tic Tac Toe - In Game</title>
    </head>
    <body>
        <div id="game-container">
            <div class="row-flex-container grid-row">
                <div class="square" data-index="0">X</div>
                <div class="square" data-index="1">X</div>
                <div class="square" data-index="2">X</div>
            </div>
            <div class="row-flex-container grid-row">
                <div class="square" data-index="3">X</div>
                <div class="square" data-index="4">X</div>
                <div class="square" data-index="5">X</div>
            </div>
            <div class="row-flex-container grid-row">
                <div class="square" data-index="6">X</div>
                <div class="square" data-index="7">X</div>
                <div class="square" data-index="8">X</div>
            </div>
        </div>
    </body>
</html>
